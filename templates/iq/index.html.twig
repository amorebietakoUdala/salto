{% extends 'base.html.twig' %}

{% block title %}{{ 'app.title.iq.index' | trans }}{% endblock %}

{% block body %}
	<main class="container-fluid" {{ stimulus_controller('modal-show') }} data-action="hidden.bs.modal->modal-show#resetModal">
        <div id="content" class="container-fluid" {{ stimulus_controller('table', 
                { 'url': path('iq_index'),
                'exportName': 'iqs',
            })}} 
            {# data-action="entity:success->table#refreshContent"  #}
            data-table-target="content"
            >
            {{ include('iq/_list.html.twig', { 
                'iqs': iqs,
                'pageSize' : pageSize,
                'page': page,
                'sortName': sortName,
                'sortOrder': sortOrder,
            }) }}
        </div>
        {{ include('iq/_spinnerModal.html.twig') }}
	</main>
{% endblock %}