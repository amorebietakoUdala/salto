{% if audits is defined and audits|length > 0 %}
<div>
   <table class="table"
      data-toggle="table"
      data-table-target="table"
      data-page-number="{{ page }}" 
      data-page-size="{{ pageSize }}"
      data-page-sortName="{{ sortName }}"
      data-page-sortOrder="{{ sortOrder }}">
      <thead>
         <tr>
            <th data-sortable="true">{{ 'audit.id' | trans }}</th>
            <th data-sortable="true">{{ 'audit.user' | trans }}</th>
            <th data-sortable="true">{{ 'audit.date' | trans }}</th>
            <th data-sortable="true">{{ 'audit.element' | trans }}</th>
            <th data-sortable="true">{{ 'audit.operation' | trans }}</th>
            <th data-sortable="true">{{ 'audit.result' | trans }}</th>
         </tr>
      </thead>
      <tbody>
         {% for audit in audits %}
            <tr>
               <td>{{ audit.id }}</td>
               <td>{{ audit.user.username }}</td>
               <td>{{ audit.date | date('Y-m-d H:i:s') }}</td>
               <td>{{ audit.element }}</td>
               <td>{{ audit.operation | trans }}</td>
               <td>{{ audit.result }}</td>
            </tr>
         {% endfor %}
      </tbody>
   </table>
</div>
{% else %}
   <div class="alert alert-secondary mt-3" role="alert">{{"messages.results0" | trans }}</div>
{% endif %}
{{ include('common/_alert.html.twig') }}